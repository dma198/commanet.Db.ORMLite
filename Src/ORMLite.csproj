<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>commanet.Db.ORMLite</AssemblyName>
    <RootNamespace>commanet.Db</RootNamespace>
    <Description>Object Oriented SQL Databases Access Package</Description>
    <Product>commanet</Product>
    <Version>2.0.0.0</Version>
    <Authors>Dmitry Molodtsov</Authors>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="*">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="NLog" Version="*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../../Db/Src/Commanet.Db.csproj" />
  </ItemGroup>

</Project>
